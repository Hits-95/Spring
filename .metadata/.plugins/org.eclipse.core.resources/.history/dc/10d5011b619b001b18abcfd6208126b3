public class FileUploadForm {

	@RequestMapping("/fileform")
	public String showUploadForm(){
		return "file_form";
		
	}
	
	@RequestMapping(path = "/uploadimage", method = RequestMethod.POST)
	public String fileUpload(){
		System.out.println("upload called...");
		return "";
	}
}
